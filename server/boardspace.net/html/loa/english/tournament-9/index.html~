win = null; 
function launchwith(file) 
{ 
  if(win == null) { 
    win = window.open("", "appletWindow", "width=600,height=480,status=yes"); 
      } 
    win.document.write("<HTML><HEAD><TITLE>Lines of Action</TITLE></HEAD>"); 
    win.document.writeln("<BODY>"); 
   win.document.writeln("<applet code=\"loa/ui/LoaApplet.class\" name=\"LoaApplet\" codebase=\"../java/\" width=550 height=450>");
   win.document.writeln("<PARAM NAME=\"filename\" VALUE=\"../../loagames/"+file+".sgf\">");
//   win.document.writeln("<param name=\"gamename\" value=\"LoA Board " + name + "\">");
    win.document.writeln("<param name=\"panel\" value=\"true\">");
    win.document.writeln("<param name=\"flags\" value=\"0\">");
//	win.document.writeln("<param name=debug value=true>");
    win.document.writeln("If you are seeing this message, your browser is not Java-Enabled.  There's");
    win.document.writeln("still hope, if you can run the java viewer standalone on your machine.");
    win.document.writeln("See the <A HREF=\"Programs.html#java\">Java Programs</A> page.");
    win.document.writeln("</applet>");
    win.document.writeln("</body>");
   win.document.close();}

  </script>
